    "@angular/compiler": "^4.3.6",
    "@angular/compiler-cli": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/platform-server": "^4.3.6",
    "@angular/router": "^4.3.6",
    "@angular/upgrade": "^4.3.6",
    "@types/jquery": "^2.0.41",
    "angular2-useful-swiper": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.16.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "express-jwt": "^5.1.0",
    "express-session": "^1.15.0",
    "fs": "0.0.1-security",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.8.1",
    "multer": "^1.3.0",
    "mysql": "^2.14.1",
