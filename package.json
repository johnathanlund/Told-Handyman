{
  "name": "told-handyman",
  "version": "1.0.0",
  "author": "Johnathan Lund",
  "description": "Told Hanyman single page website.",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server --baseDir ./src --baseDir ./\" ",
    "lite": "lite-server",
    "lite2": "lite-server --baseDir ./src --baseDir ./",
    "lite3": "lite-server --baseDir ./src --baseDir ./tsconfig.json --baseDir ./",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "be": "nodemon src/server/server.js",
    "ngc": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "rollup": "node_modules/.bin/rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "build:aot": "node_modules/.bin/ngc -p tsconfig-aot.json && node_modules/.bin/rollup -c rollup-config.js"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
